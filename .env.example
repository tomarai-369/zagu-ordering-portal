# Kintone Environment
KINTONE_BASE_URL=https://your-subdomain.kintone.com

# App IDs
KINTONE_PRODUCTS_APP_ID=1
KINTONE_DEALERS_APP_ID=2
KINTONE_ORDERS_APP_ID=3

# API Tokens (generate from each app's settings)
KINTONE_PRODUCTS_TOKEN=your_products_api_token
KINTONE_DEALERS_TOKEN=your_dealers_api_token
KINTONE_ORDERS_TOKEN=your_orders_api_token

# Server
PORT=3001
NODE_ENV=development
